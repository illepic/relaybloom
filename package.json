{
  "name": "RELAYbloom",
  "version": "0.1.0",
  "description": "RELAYbloom race app",
  "repository": "https://github.com/illepic/relaybloom",
  "author": "Christopher Bloom",
  "private": true,
  "src": "src",
  "test": "test",
  "dist": "dist",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "TARGET=build webpack",
    "winbuild": "set TARGET=build&& webpack",
    "start": "TARGET=dev webpack-dev-server --progress --colors --hot --inline --history-api-fallback --host 0.0.0.0 --content-base build/",
    "winstart": "set TARGET=dev&&webpack-dev-server --devtool eval --progress --colors --hot --inline --history-api-fallback --content-base build/",
    "postinstall": "cp -a ./node_modules/bootswatch/bower_components/bootstrap/fonts ./node_modules/bootswatch",
    "express-start": "node relaybloomserver"
  },
  "dependencies": {
    "bootswatch": "^3.3.5",
    "express": "^4.13.3",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "moment-duration-format": "^1.3.0",
    "offline-js": "^0.7.11",
    "parse": "^1.5.0",
    "parse-react": "^0.4.2",
    "react": "^0.13.3",
    "react-bootstrap": "^0.24.3",
    "react-router": "^0.13.3",
    "react-router-bootstrap": "^0.18.0",
    "socket.io": "^1.3.6",
    "socket.io-client": "^1.3.6",
    "store": "^1.3.17"
  },
  "devDependencies": {
    "babel-core": "^5.8.21",
    "babel-loader": "^5.3.2",
    "classnames": "^2.1.3",
    "css-loader": "^0.15.6",
    "exports-loader": "^0.6.2",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.1",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "react-hot-loader": "^1.2.8",
    "sass-loader": "^2.0.0",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1",
    "webpack-merge": "^0.1.2"
  }
}
